<table class="table">
  <thead>
    <tr>
      <th>Phone</th>
      <th>Called at</th>
      <th>Duration</th>
      <th>Internal</th>
      <th>Call type</th>
      <th>Rings</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%# <%= render calls %>
    <% calls.each do |call| %>
      <%= react_component "Call", { call: call } %>
    <% end %>
  </tbody>
</table>
